{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UseTestFlightData": true,
  "FlightSearch": {
    "Kiwi": {
      "ApiKey": "",
      "BaseUrl": "https://tequila-api.kiwi.com",
      "DefaultCurrency": "USD",
      "TimeoutSeconds": 30
    },
    "Amadeus": {
      "ApiKey": "",
      "ApiSecret": "",
      "BaseUrl": "https://test.api.amadeus.com",
      "DefaultCurrency": "USD",
      "TimeoutSeconds": 30
    }
  },
  "Cache": {
    "Enabled": true,
    "FlightSearchCacheTtlMinutes": 10
  },
  "RateLimit": {
    "TokenLimit": 100,
    "TokensPerPeriod": 20,
    "ReplenishmentPeriodSeconds": 60,
    "QueueLimit": 2,
    "RetryAfterSeconds": 60
  }
}
